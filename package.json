{
  "name": "easeljs",
  "version": "2.0.0",
  "description": "The Easel JavaScript library provides a full, hierarchical display list, a core interaction model, and helper classes to make working with the HTML5 Canvas element much easier. Part of the CreateJS suite of libraries.",
  "keywords": [
    "html5",
    "webgl",
    "canvas",
    "easeljs",
    "createjs",
    "gskinner",
    "flash",
    "animate",
    "spritesheet"
  ],
  "author": "gskinner.com, inc.",
  "homepage": "https://createjs.com/easeljs",
  "url": "https://npmjs.com/package/easeljs",
  "bugs": "https://github.com/createjs/easeljs/issues",
  "license": "MIT",
  "contributors": [
    "Grant Skinner <grant@gskinner.com> (https://github.com/gskinner)",
    "Lanny McNie <lanny@gskinner.com> (https://github.com/lannymcnie)",
    "Wes Gorgichuk <wes@gskinner.com> (https://github.com/wdamien)",
    "Mike Chambers <> (https://github.com/mikechambers)",
    "Patrick McGuckin <patrick@gskinner.com> (https://github.com/tehvgg)",
    "David Gillen <david@gskinner.com> (https://github.com/davidhgillen)"
  ],
  "main": "dist/easel.common.js",
  "jsnext:main": "dist/easel.es6.js",
  "jsdelivr": "dist/easel.min.js",
  "logo": "icon.png",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/createjs/easeljs.git"
  },
  "npmName": "easeljs",
  "npmFileMap": [
    {
      "basePath": "dist",
      "files": [
        "**/*"
      ]
    }
  ],
  "directories": {
    "doc": "docs",
    "example": "examples",
    "lib": "dist",
    "test": "tests"
  },
  "scripts": {
    "test": "cd node_modules/createjs && npm run test",
    "test:headless": "cd node_modules/createjs && npm run test:headless",
    "dev": "cd node_modules/createjs && npm run dev",
    "build": "cd node_modules/createjs && npm run build",
    "build:next": "cd node_modules/createjs && npm run build:next",
    "plugins": "cd node_modules/createjs && npm run plugins",
    "docs": "cd node_modules/createjs && npm run docs"
  },
  "dependencies": {
    "createjs": "2.0.0",
    "tweenjs": "2.0.0"
  },
  "devDependencies": {
    "@babel/plugin-external-helpers": "^7.0.0-beta.31",
    "@babel/preset-env": "^7.0.0-beta.31",
    "js-imagediff": "tehvgg/js-imagediff"
  },
  "engine": "node >= 7.10.1"
}
