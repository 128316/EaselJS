{
  "name": "@createjs/easeljs",
  "version": "2.0.0",
  "description": "The Easel JavaScript library provides a full, hierarchical display list, a core interaction model, and helper classes to make working with the HTML5 Canvas element much easier. Part of the CreateJS suite of libraries.",
  "keywords": [
    "html5",
    "webgl",
    "canvas",
    "easeljs",
    "createjs",
    "gskinner",
    "flash",
    "animate",
    "spritesheet"
  ],
  "homepage": "https://createjs.com/easeljs",
  "url": "https://npmjs.com/package/@createjs/easeljs",
  "bugs": "https://github.com/createjs/easeljs/issues",
  "license": "MIT",
  "contributors": [
    "Grant Skinner <grant@gskinner.com> (https://github.com/gskinner)",
    "Lanny McNie <lanny@gskinner.com> (https://github.com/lannymcnie)",
    "Wes Gorgichuk <wes@gskinner.com> (https://github.com/wdamien)",
    "Mike Chambers <> (https://github.com/mikechambers)",
    "Patrick McGuckin <patrick@gskinner.com> (https://github.com/tehvgg)",
    "David Gillen <david@gskinner.com> (https://github.com/davidhgillen)"
  ],
  "main": "dist/easeljs.common.js",
  "jsnext:main": "dist/easeljs.es.js",
  "jsdelivr": "dist/easeljs.min.js",
  "logo": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/createjs/easeljs.git"
  },
  "npmName": "easeljs",
  "npmFileMap": [
    {
      "basePath": "dist",
      "files": [
        "**/*"
      ]
    }
  ],
  "directories": {
    "doc": "docs",
    "example": "examples",
    "lib": "dist",
    "test": "tests"
  },
  "scripts": {
    "lint": "cd node_modules/@createjs/build && npm run lint",
    "test": "cd node_modules/@createjs/build && npm test",
    "dev": "cd node_modules/@createjs/build && npm run dev",
    "build:next": "cd node_modules/@createjs/build && npm run build:next",
    "build:plugins": "cd node_modules/@createjs/build && npm run build:plugins",
    "build:dist": "cd node_modules/@createjs/build && npm run build:dist",
    "docs": "cd node_modules/@createjs/docs && npm run gen"
  },
  "dependencies": {

  },
  "devDependencies": {
    "js-imagediff": "tehvgg/js-imagediff"
  },
  "engine": "node >= 7.10.1"
}
